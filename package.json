{
  "name": "EnviroLink",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:preview": "eas build -p android --profile preview",
    "build:production": "eas build -p android --profile production",
    "build:ios": "eas build -p ios --profile production",
    "build:all": "eas build --platform all --profile production",
    "build:minisized": "npm run optimize && eas build -p android --profile minisized",
    "build:ultra-minimal": "npm run optimize && eas build -p android --profile minisized --non-interactive",
    "build:ultra-minimal-apk": "npm run optimize && eas build -p android --profile ultra-minimal --non-interactive --local",
    "clean:all": "rm -rf node_modules && npx react-native-clean-project --keep-node-modules && npm cache clean --force",
    "submit:android": "eas submit -p android --latest",
    "submit:ios": "eas submit -p ios --latest",
    "deploy:update": "eas update --branch production",
    "optimize": "node optimize.js",
    "clean": "rimraf node_modules",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@google/generative-ai": "^0.24.0",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.39.3",
    "expo": "~52.0.42",
    "expo-blur": "^14.0.3",
    "expo-constants": "^17.0.5",
    "expo-font": "^13.0.3",
    "expo-haptics": "^14.0.1",
    "expo-image": "~2.0.7",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-network": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "^2.0.1",
    "expo-symbols": "^0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "^0.27.4",
    "expo-web-browser": "^14.0.2",
    "lucide-react-native": "^0.475.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "expo-camera": "~16.0.18"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "react-native-clean-project": "^4.0.3",
    "sharp": "^0.34.0",
    "typescript": "^5.3.3"
  }
}